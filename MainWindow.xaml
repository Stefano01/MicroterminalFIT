<Window x:Class="MicroTerminalFIT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MicroTerminalFIT"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="IniciarComunicacao" Content="Ativar" HorizontalAlignment="Left" Margin="26,19,0,0" VerticalAlignment="Top" Click="IComunicacao_Click"/>
        <Button x:Name="FecharComunicacao" Content="Desativar" HorizontalAlignment="Left" Margin="77,19,0,0" VerticalAlignment="Top" Click="FComunicacao_Click"/>

    </Grid>
</Window>
